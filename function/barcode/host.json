{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "functionTimeout": "00:10:00",
  "cors": {
    "allowedOrigins": [
      "https://__SWA_HOSTNAME__.azurestaticapps.net",
      "http://localhost:5173",
      "http://localhost:3000"
    ],
    "supportedMethods": ["GET", "POST", "PUT", "OPTIONS"]
  }
}